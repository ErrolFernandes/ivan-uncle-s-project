<html>
    <%- include("./partials/header.ejs") %>
    <body>
        <section class="container section scrollspy" id="service">
            <div class="row">
              <div class="col s12 l12">
                <h2 class="indigo-text text-darken-4" >Our Clients</h2>
             <ul class="collection" id="myList">
            <% datas.forEach(data => { %>
                <% if (data.catg === 'client') { %>  
                  <li class="collection-item"> <%= data.name %> </li>
                  <%}%>
              <% }) %>  
            </ul>
        </section>
       
       <!-- footer   -->
       <%- include("./partials/footer.ejs") %>
    </body>
</html>
